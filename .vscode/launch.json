{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "alqmc-helper",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "env": {
        "NODE_ENV": "dev"
      },
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/packages/vscode-extension/dist",
        "--disable-extensions"
      ]
    }
  ]
}

